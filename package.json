{
  "name": "shop-backend-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "package:dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "package:build": "pnpm -r --filter='./packages/*' run build",
    "dev": "next dev -p 3080",
    "build": "next build",
    "start": "next start -p 3080",
    "lint": "next lint",
    "format": "prettier -w ./src"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prisma": "6.0.1",
    "tailwindcss": "^3.4.17",
    "tsc-alias": "^1.8.10"
  },
  "dependencies": {
    "@prisma/client": "6.0.1",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8.57.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  }
}
